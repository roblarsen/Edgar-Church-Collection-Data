{
  "name": "edgar-church-collection-webapp",
  "version": "1.0.0",
  "description": "TypeScript web application for browsing the Edgar Church comic book collection data",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "nodemon --exec ts-node src/server.ts",
    "build:client": "tsc src/app.ts --outDir public --target ES2020 --lib ES2020,DOM --skipLibCheck",
    "build:all": "npm run build && npm run build:client",
    "postinstall": "npm run build:all"
  },
  "keywords": [
    "edgar-church",
    "comic-books",
    "data-visualization",
    "ag-grid",
    "typescript"
  ],
  "author": "Edgar Church Collection Data Project",
  "license": "CC-BY-4.0",
  "dependencies": {
    "ag-grid-community": "^34.1.2",
    "cors": "^2.8.5",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
